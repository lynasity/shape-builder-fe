{
  "name": "canva-apps-sdk-starter-kit",
  "description": "A boilerplate for creating Canva Apps.",
  "engines": {
    "node": "^18 || ^20.10.0",
    "npm": "^9 || ^10"
  },
  "scripts": {
    "start": "ts-node ./scripts/start.ts",
    "extract": "formatjs extract 'src/**/*.{ts,tsx}' --out-file dist/messages_en.json",
    "build": "webpack --config webpack.config.js --mode production && npm run extract",
    "lint:types": "tsc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier {src,utils}/**/*.ts* --no-config --write",
    "format:check": "prettier {src,utils}/**/*.ts* --no-config --check --ignore-path",
    "format:file": "prettier $1 --no-config --write",
    "test": "jest --no-cache",
    "test:watch": "jest --no-cache --watch"
  },
  "keywords": [],
  "author": "Canva Pty Ltd.",
  "license": "SEE LICENSE IN LICENSE.md",
  "private": true,
  "dependencies": {
    "@canva/app-i18n-kit": "^0.0.1-beta.5",
    "@canva/app-ui-kit": "^4.3.0",
    "@canva/asset": "^2.0.0",
    "@canva/design": "^2.2.1",
    "@canva/error": "^2.0.0",
    "@canva/intents": "^2.0.0",
    "@canva/platform": "^2.0.0",
    "@canva/user": "^2.0.0",
    "@types/paper": "^0.11.14",
    "clsx": "^1.2.1",
    "fabric": "^6.3.0",
    "firebase": "^10.13.0",
    "paper": "^0.12.18",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-icons": "^5.3.0"
  },
  "devDependencies": {
    "@formatjs/ts-transformer": "^3.13.17",
    "@svgr/webpack": "^8.0.1",
    "@types/cors": "^2.8.13",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.4.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.7.14",
    "@types/node-fetch": "^2.6.2",
    "@types/nodemon": "^1.19.2",
    "@types/react-dom": "^18.1.0",
    "@types/webpack-env": "^1.18.0",
    "chalk": "^4.1.2",
    "cli-table3": "^0.6.2",
    "cors": "^2.8.5",
    "css-loader": "^6.7.1",
    "css-modules-typescript-loader": "^4.0.1",
    "cssnano": "^6.0.1",
    "debug": "^4.3.4",
    "dotenv": "^16.0.1",
    "exponential-backoff": "^3.1.0",
    "express": "^4.18.1",
    "express-basic-auth": "^1.2.1",
    "jest": "^29.4.2",
    "jsonwebtoken": "^9.0.0",
    "jwks-rsa": "^3.0.1",
    "mini-css-extract-plugin": "^2.6.1",
    "ngrok": "^4.3.3",
    "node-fetch": "^2.6.7",
    "nodemon": "^2.0.19",
    "postcss-loader": "^7.3.3",
    "prettier": "^2.7.1",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.5",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^4.8.2",
